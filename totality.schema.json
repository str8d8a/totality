{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "observation",
  "description": "A document describing the observation of one or more items, by a particular observer using a specified method. In epistemic terms, the submission of this document is an attestation that the items are as described, and the reliability of this attestation is a function of various factors.\n\nThe `observer`, `method`, and `items` properties are all required for the observation document to be valid.",
  "type": "object",
  "required": [
    "observer",
    "method",
    "items"
  ],
  "additionalProperties": false,
  "properties": {
    "observer": {
      "type": "object",
      "title": "observer",
      "additionalProperties": false,
      "properties": {
        "fullName": {
          "type": "string"
        },
        "email": {
          "type": "string",
          "format": "email"
        },
        "userId": {
          "type": "string"
        }
      }
    },
    "method": {
      "type": "object",
      "title": "observation method",
      "additionalProperties": false,
      "properties": {
        "mode": {
          "type": "string",
          "enum": [
            "eye",
            "web",
            "camera",
            "microphone",
            "transponder",
            "lidar"
          ]
        },
        "platform": {
          "description": "The mounting point of the sensory system used to collect the observation",
          "type": "string",
          "enum": [
            "direct",
            "handheld",
            "stationary",
            "terrestrial vehicle vehicle",
            "aerial",
            "orbital"
          ]
        },
        "identity": {
          "description": "The relationship of the submitter of this observation to the data collection",
          "type": "string",
          "enum": [
            "self",
            "3rdparty"
          ]
        },
        "encoding": {
          "type": "string",
          "enum": [
            "human",
            "standard-based",
            "algorithmic"
          ]
        }
      }
    },
    "items": {
      "type": "array",
      "items": {
        "type": "object",
        "required": [
          "time",
          "location",
          "data"
        ],
        "additionalProperties": false,
        "properties": {
          "time": {
            "type": "string",
            "format": "date-time"
          },
          "location": {
            "description": "A point on Earth defined by latitude and longitude",
            "type": "object",
            "properties": {
              "latitude": {
                "type": "number",
                "minimum": -90,
                "maximum": 90
              },
              "longitude": {
                "type": "number",
                "minimum": -180,
                "maximum": 180
              }
            }
          },
          "data": {
            "anyOf": [
              {
                "additionalProperties": false,
